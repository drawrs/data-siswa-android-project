<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khilman.www.aplikasidatasiswa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.khilman.www.aplikasidatasiswa.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.khilman.www.aplikasidatasiswa.TambahActivity"
            android:label="Tambah Data"
            android:parentActivityName="com.khilman.www.aplikasidatasiswa.MainActivity" />
        <activity
            android:name="com.khilman.www.aplikasidatasiswa.UbahActivity"
            android:label="Ubah Data"
            android:parentActivityName="com.khilman.www.aplikasidatasiswa.MainActivity" />
    </application>

</manifest>